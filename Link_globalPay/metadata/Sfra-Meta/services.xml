    <service service-id="int_globalpay.http.generic">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>GlobalPay</log-prefix>
        <comm-log-enabled>false</comm-log-enabled>
        <force-prd-enabled>true</force-prd-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>int_globalpay.http.generic</profile-id>
        <credential-id>int_globalpay.http.generic</credential-id>
    </service>

</services>
