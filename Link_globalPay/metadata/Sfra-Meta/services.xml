    <service-credential service-credential-id="int_globalpay.http.generic">
        <url>https://apis.sandbox.globalpay.com/ucp</url>
        <user-id/>
    </service-credential>

    <service-profile service-profile-id="CybersourceProfile">
        <timeout-millis>30000</timeout-millis>
        <rate-limit-enabled>false</rate-limit-enabled>
        <rate-limit-calls>0</rate-limit-calls>
        <rate-limit-millis>0</rate-limit-millis>
        <cb-enabled>false</cb-enabled>
        <cb-calls>0</cb-calls>
        <cb-millis>0</cb-millis>
    </service-profile>

    <service-profile service-profile-id="GlobalPay">
        <timeout-millis>0</timeout-millis>
        <rate-limit-enabled>false</rate-limit-enabled>
        <rate-limit-calls>0</rate-limit-calls>
        <rate-limit-millis>0</rate-limit-millis>
        <cb-enabled>false</cb-enabled>
        <cb-calls>0</cb-calls>
        <cb-millis>0</cb-millis>
    </service-profile>

    <service-profile service-profile-id="int_globalpay.http.generic">
        <timeout-millis>0</timeout-millis>
        <rate-limit-enabled>false</rate-limit-enabled>
        <rate-limit-calls>0</rate-limit-calls>
        <rate-limit-millis>0</rate-limit-millis>
        <cb-enabled>false</cb-enabled>
        <cb-calls>0</cb-calls>
        <cb-millis>0</cb-millis>
    </service-profile>

    <service service-id="GlobalPay">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>GlobalPay</log-prefix>
        <comm-log-enabled>false</comm-log-enabled>
        <force-prd-enabled>true</force-prd-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
