<?xml version="1.0" encoding="UTF-8"?>
<services xmlns="http://www.demandware.com/xml/impex/services/2014-09-26">
    <service-credential service-credential-id="GlobalPay">
        <url>https://apis.sandbox.globalpay.com/ucp</url>
        <user-id/>
