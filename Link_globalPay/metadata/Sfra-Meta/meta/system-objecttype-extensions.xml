    <type-extension type-id="SitePreferences">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="gp_app_id">
                <display-name xml:lang="x-default">GP App Id</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="gp_app_key">
                <display-name xml:lang="x-default">GP App Key</display-name>
                <type>password</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="gp_captureMode">
                <display-name xml:lang="x-default">GP captureMode</display-name>
                <type>enum-of-string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition>
                        <display xml:lang="x-default">AUTO</display>
                        <value>AUTO</value>
                    </value-definition>
                    <value-definition default="true">
                        <display xml:lang="x-default">LATER</display>
                        <value>LATER</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
            <attribute-definition attribute-id="gp_clientID">
                <display-name xml:lang="x-default">GP clientID</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="gp_enable_applepay">
                <display-name xml:lang="x-default">GP Enable Apple Pay</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="gp_enable_googlepay">
                <display-name xml:lang="x-default">GP Enable Google Pay</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="gp_enable_paypal">
                <display-name xml:lang="x-default">GP Enable PayPal</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="gp_env">
                <display-name xml:lang="x-default">GP Env</display-name>
                <type>enum-of-string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition default="true">
                        <display xml:lang="x-default">sandbox</display>
                        <value>sandbox</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">production</display>
                        <value>production</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
            <attribute-definition attribute-id="gp_gatewayMerchantId">
                <display-name xml:lang="x-default">gp Gateway MerchantId</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="gp_gpayEnv">
                <display-name xml:lang="x-default">gp google Env</display-name>
                <type>enum-of-string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition>
                        <display xml:lang="x-default">TEST</display>
                        <value>TEST</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">PRODUCTION</display>
                        <value>PRODUCTION</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
            <attribute-definition attribute-id="gp_gpayMerchantId">
                <display-name xml:lang="x-default">gp MerchantId</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="gp_gpayMerchantName">
                <display-name xml:lang="x-default">gp MerchantName</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>  
        </custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="gp_globalpay">
                <display-name xml:lang="x-default">GlobalPay</display-name>
                <attribute attribute-id="gp_app_id"/>
                <attribute attribute-id="gp_clientID"/>
                <attribute attribute-id="gp_app_key"/>
                <attribute attribute-id="gp_enable_googlepay"/>
                <attribute attribute-id="gp_enable_applepay"/>
                <attribute attribute-id="gp_enable_paypal"/>
                <attribute attribute-id="gp_captureMode"/>
                <attribute attribute-id="gp_gatewayMerchantId"/>
                <attribute attribute-id="gp_gpayMerchantId"/>
                <attribute attribute-id="gp_gpayMerchantName"/>
                <attribute attribute-id="gp_gpayEnv"/>
                <attribute attribute-id="gp_env"/>
            </attribute-group>
        </group-definitions>
    </type-extension>

</metadata>
